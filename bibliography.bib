@book{schumacher_orte_2023,
	address = {Berlin/Heidelberg},
	series = {Digitale {Literaturwissenschaft}},
	title = {Orte und {Räume} im {Roman} {Ein} {Beitrag} zur digitalen {Literaturwissenschaft}},
	isbn = {ISBN 978-3-662-66034-8            ISBN 978-3-662-66035-5 (eBook)},
	url = {https://link.springer.com/book/10.1007/978-3-662-66035-5},
	abstract = {Dieses Open-Access-Buch bietet eine breit angelegte, digital unterstützte, korpusbasierte Studie zur Referenzierung von Orten und Räumen in Erzähltexten. Aus literaturwissenschaftlicher, insbesondere narratologischer Forschung sowie mathematischen, philosophischen, physikalischen und kulturwissenschaftlichen Ansätzen zur Thematik des Raumes wird ein fuzzy-set-Modell herausgearbeitet, mit dem Raum in literarischen Texten analysiert und quantifiziert werden kann. Das Modell ist Grundlage eines Machine-Learning- Trainings, mit Hilfe dessen ein Tool trainiert wurde, das Ausdrücke, die in die Kategorien des theoriebasierten Modells fallen, automatisch erkennt und annotiert. In einem Kernkorpus aus 100 Romanen aus vier Jahrhunderten (18-21) wurden mit Hilfe dieses Tools mehr als eine Million Annotationen in die Texte eingefügt und anschließend analysiert.},
	language = {Deutsch},
	urldate = {2023-06-01},
	publisher = {J.B. Metzler},
	author = {Schumacher, Mareike K.},
	year = {2023}
}

